{
    "manifest_version": 3,
    "name": "Custom New Tab",
    "version": "1.0",
    "description": "Thay thế trang New Tab bằng HTML tùy chỉnh.",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    }
}