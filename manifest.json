{
    "manifest_version": 3,
    "name": "Custom New Tab",
    "version": "1.0",
    "description": "Custom new tab page",
    "permissions": [
        "storage",
        "bookmarks",
        "history",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Custom New Tab"
    },
    "icons": {
        "128": "icon.png"
    }
}