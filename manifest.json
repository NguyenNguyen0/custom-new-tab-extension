{
    "manifest_version": 3,
    "name": "Custom NewTab Override",
    "version": "1.0",
    "description": "Override the new tab page with your custom HTML design",
    "permissions": ["storage", "tabs"],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Custom NewTab Override"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}